<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kr.co.digigroove.qrboard_tool.dao.ContentBackgroundDAO">

    <!-- 광고 콘텐츠 배경화면 등록 -->
    <insert id="insertContentBackgroundEntity" parameterType="contentBackgroundEntity">
        INSERT INTO content_background (
            content_idx
            , content_background_color
        ) VALUES (
            #{contentIdx}
            , #{contentBackgroundColor}
        )
    </insert>

</mapper>
